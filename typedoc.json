{
  "name": "KeyframeKit",
	"plugin": ["typedoc-plugin-param-names", "typedoc-plugin-mdn-links", "typedoc-plugin-inline-sources", "typedoc-plugin-markdown", "typedoc-plugin-remark", "typedoc-github-wiki-theme", "typedoc-plugin-missing-exports"],

  "entryPoints": ["src/KeyframeKit.ts"],

  "excludeExternals": true,
  "placeInternalsInOwningModule": true,
  
  //"flattenOutputFiles": false,

  "router": "member", // "module"

  "sidebar": {
    //"autoConfiguration": false,
    "heading": "Reference"
  },

  "remarkPlugins": [
    ["remark-insert-headings", { "text": "Contents" }],
    ["remark-toc", { "maxDepth": 3 }],
    //["remark-collapse", {"test": "Contents"}]
  ],

  "modulesFileName": "Reference",

  "expandParameters": true,
  "expandObjects": true,

  //"parametersFormat": "htmlTable",

  "useCodeBlocks": true,

  "hidePageTitle": true,

  "namedParamName": "obj"
}