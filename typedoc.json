{
  "name": "KeyframeKit",
	"plugin": ["typedoc-plugin-mdn-links", "typedoc-plugin-inline-sources", "typedoc-plugin-markdown", "typedoc-plugin-remark", "typedoc-github-wiki-theme", "typedoc-plugin-missing-exports"],

  "entryPoints": ["src/KeyframeKit.ts"],

  "excludeExternals": true,
  "placeInternalsInOwningModule": true,
  
  //"flattenOutputFiles": false,

  "router": "member",

  "sidebar": {
    //"autoConfiguration": false,
    "heading": "Reference"
  },

  /*
  "remarkPlugins": [
    ["remark-insert-headings", { "text": "Contents" }],
    ["remark-toc", { "maxDepth": 2 }]
  ],*/

  "modulesFileName": "Reference",

  "expandParameters": true,
  "expandObjects": true,

  "parametersFormat": "htmlTable",
  "interfacePropertiesFormat": "htmlTable",
  "classPropertiesFormat": "htmlTable",
  "typeAliasPropertiesFormat": "htmlTable",
  "propertyMembersFormat": "htmlTable",
  "typeDeclarationFormat": "htmlTable",

  "tableColumnSettings": {
    "hideDefaults": true,
    "hideInherited": true,
    "hideModifiers": true,
    "hideOverrides": true,
    "hideSources": true,
    "hideValues": true,
    "leftAlignHeaders": true
  },

  "useCodeBlocks": true,

  "hidePageTitle": true
}