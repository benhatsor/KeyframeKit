{
  "name": "KeyframeKit",
  
	"plugin": [
    "typedoc-plugin-param-names",
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-inline-sources",
    "typedoc-plugin-markdown",
    "typedoc-plugin-remark",
    "typedoc-github-wiki-theme",
    "typedoc-plugin-missing-exports"
  ],

  "entryPoints": ["src/KeyframeKit.ts"],

  "excludeExternals": true,
  "internalModule": "_internal_",
  //"placeInternalsInOwningModule": true,
  
  //"flattenOutputFiles": false,

  "router": "member", // "module"

  "readme": "none",

  "sidebar": {
    //"autoConfiguration": false,
    "heading": "[KeyframeKit](../wiki)"
  },

  "remarkPlugins": [
    ["remark-insert-headings", { "text": "Contents", "minHeadingCount": 2 }],
    ["remark-toc", { "maxDepth": 3, "tight": false }],
    //["remark-collapse", {"test": "Contents"}]
  ],

  "modulesFileName": "KeyframeKit",

  "expandParameters": true,
  "expandObjects": true,

  //"parametersFormat": "htmlTable",

  "useCodeBlocks": true,

  "hidePageTitle": true,

  "namedParamName": "obj"
}